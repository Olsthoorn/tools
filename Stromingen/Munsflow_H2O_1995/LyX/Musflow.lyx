\begin_layout Standard
The Heinen,
 Bakker and Wösten (2018) relation for conductivity which uses a different conductivity model with an extra parameter 
\begin_inset Formula $\lambda$
\end_inset

 while dropping the 
\begin_inset Quotes pld
\end_inset

el
\begin_inset Quotes prd
\end_inset

 
\begin_inset Formula $l$
\end_inset

 parameter in VG-M
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
K\left(\psi\right) & =K_{s}\frac{\left(\left(1+\left(\alpha\psi\right)^{n}\right)^{m}-\left(\alpha\psi\right)^{n-1}\right)^{2}}{\left(1+\left(\alpha\psi\right)^{n}\right)^{m\left(\lambda+2\right)}}\\
 & =K_{s}\left[\left(1+\left(\alpha\psi\right)^{n}\right)^{-m}\right]^{\left(\lambda+2\right)}\left(\left(1+\left(\alpha\psi\right)^{n}\right)^{m}-\left(\alpha\psi\right)^{n-1}\right)^{2}\\
 & =K_{s}S^{\left(\lambda+2\right)}\left(S^{-1}-\left(\alpha\psi\right)^{n-1}\right)^{2}\\
 & =K_{s}S^{\left(\lambda+2\right)}\left(S^{-1}-\left(S^{-\frac{1}{m}}-1\right)^{\frac{n-1}{n}}\right)^{2}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
with
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
S & =\left(1+\left(\alpha\psi\right)^{n}\right)^{-m}\\
S^{-1} & =\left(1+\left(\alpha\psi\right)^{n}\right)^{+m}\\
\left(1+\left(\alpha\psi\right)^{n}\right)^{+m} & =S^{-1}\\
\left(\alpha\psi\right)^{n} & =S^{-\frac{1}{m}}-1\\
\left(\alpha\psi\right)^{n-1} & =\left(S^{-\frac{1}{m}}-1\right)^{\frac{n-1}{n}}
\end{align*}

\end_inset


\end_layout

