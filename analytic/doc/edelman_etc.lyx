waarbij en
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\iop^{0}\erfc\left(u\right)=\erfc\left(u\right)\;\text{en\;}\iop^{-1}\erfc\left(u\right)=\frac{2}{\sqrt{\pi}}e^{-u^{2}}\label{eq:i0erfc0-eni-1erfc}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
