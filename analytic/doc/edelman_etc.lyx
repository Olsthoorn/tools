\begin_layout Standard
\begin_inset Formula 
\begin{align*}
Q\left(x,t\right) & =-kD\frac{\partial\phi\left(x,t\right)}{\partial x}=-kD\frac{d}{du}\left(At^{n/2}\frac{\ierfc\left(n,u\right)}{\ierfc\left(n.0\right)}\right)\frac{\partial u}{\partial x}=kDAt^{n/2}\frac{\ierfc\left(n-1,u\right)}{\ierfc\left(n,0\right)}\sqrt{\frac{S}{4kDt}}\\
 & =At^{\left(n-1\right)/2}\sqrt{\frac{kDS}{4}}\frac{\ierfc\left(n-1,0\right)}{\ierfc\left(n,0\right)}\frac{\ierfc\left(n-1,u\right)}{\ierfc\left(n-1,0\right)}\\
 & =At^{\left(n-1\right)/2}\sqrt{\frac{kDS}{4}}\frac{F\left(n-1,u\right)}{f\left(n-1\right)}
\end{align*}

\end_inset


\end_layout

