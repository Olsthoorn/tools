\begin_layout Standard
Proefschrift Edelman (1947).
 Merk op dat 
\begin_inset Formula $\phi=\phi$
\end_inset

,
 
\begin_inset Formula $q=Q/kD$
\end_inset

,
 
\begin_inset Formula $\tau=\frac{kD}{S}t$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\phi & =-\phi_{0}f_{0}\left(u\right)=\phi_{0}\left(1-E\left(u\right)\right)\\
\phi & =-q_{0}\tau^{1/2}f_{1}\left(u\right)=-q_{0}\tau^{1/2}\left[\frac{2}{\sqrt{\pi}}e^{-u^{2}}-2u\left(1-E\left(u\right)\right)\right]\\
\phi & =-\alpha\tau f_{2}\left(u\right)=-a\tau\left[\frac{2}{\sqrt{\pi}}ue^{-u^{2}}-\left(2u^{2}+1\right)\left(1-E\left(u\right)\right)\right]\\
\phi & =-\beta\tau^{3/2}f_{3}\left(u\right)=-\frac{2}{3}\beta\tau^{3/2}\left[\frac{2}{\sqrt{\pi}}\left(u^{2}+1\right)e^{-u^{2}}-\left(2u^{3}+3u\right)\left(1-E\left(u\right)\right)\right]
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
q & =-\frac{\phi_{0}}{\sqrt{\pi}}\tau^{-1/2}e^{-u^{2}}\\
q & =q_{0}\left\{ 1-E\left(u\right)\right\} \\
q & =-a\tau^{1/2}\left[\frac{2}{\sqrt{\pi}}e^{-u^{2}}-2u\left(1-E\left(u\right)\right)\right]\\
q & =-\beta\tau\left[\frac{2}{\sqrt{\pi}}ue^{-u^{2}}-\left(2u^{2}+1\right)\left(1+E\left(u\right)\right)\right]
\end{align*}

\end_inset


\end_layout

