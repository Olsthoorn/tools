\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\erfc\left(2,u\right)=\frac{1}{4}\ierfc\left(0,u\right)-\frac{u}{2}\ierfc\left(1,u\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\ierfc\left(3,u\right)=\frac{1}{6}\ierfc\left(1,u\right)-\frac{u}{3}\ierfc\left(2,u\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\erfc\left(2,u\right) & =\frac{1}{4}\ierfc\left(0,u\right)-\frac{u}{2}\ierfc\left(1,u\right)\\
 & =\frac{1}{4}\ierfc\left(0,u\right)-\frac{u}{2}\left[\frac{1}{2}\ierfc\left(-1,u\right)-u\ierfc\left(0,u\right)\right]\\
 & =\frac{1}{4}\ierfc\left(0,u\right)-\frac{u}{4}\ierfc\left(-1,u\right)+\frac{u^{2}}{2}\ierfc\left(0,u\right)\\
 & =\left(\frac{u^{2}}{2}+\frac{1}{4}\right)\ierfc\left(0,u\right)-\frac{u}{4}\ierfc\left(-1,u\right)\\
 & =\frac{1}{4}\left[\left(2u^{2}+1\right)\ierfc\left(0,u\right)-u\ierfc\left(-1,u\right)\right]
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\ierfc\left(3,u\right) & =\frac{1}{3}\left\{ \frac{1}{2}\ierfc\left(1,u\right)-u\left[\ierfc\left(2,u\right)\right]\right\} \\
 & =\frac{1}{12}\left\{ 2\ierfc\left(1,u\right)-\left(2u^{3}+u\right)\ierfc\left(0,u\right)+u^{2}\ierfc\left(-1,u\right)\right\} \\
 & =\frac{1}{12}\left\{ \ierfc\left(-1,u\right)-2u\ierfc\left(0,u\right)-\left(2u^{3}+u\right)\ierfc\left(0,u\right)+u^{2}\ierfc\left(-1,u\right)\right\} \\
 & =\frac{1}{8}\frac{2}{3}\left\{ \left(u^{2}+1\right)\ierfc\left(-1,u\right)-\left(2u^{3}+3u\right)\ierfc\left(0,u\right)\right\} 
\end{align*}

\end_inset


\end_layout

